<h2>Lista klientów</h2>

<table>
    <thead>
    <tr>
        <th>
            E-mail:
        </th>
        <th>
            Imię:
        </th>

        <th>
            Data następnego kontaktu:
        </th>
    </tr>
    </thead>
    <tbody>
    {{#each clients}}
        <tr>
            <th>
                {{this.name}}
            </th>
            <td>
                {{this.mail}}
            </td>
            <td>
                {{this.nextContactAt}}
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>